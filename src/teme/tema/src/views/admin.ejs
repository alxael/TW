<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />

    <!-- Fonts -->
    <link rel="preconnect" href="https://fonts.googleapis.com" />
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin />
    <link
      href="https://fonts.googleapis.com/css2?family=Montserrat:ital,wght@0,100..900;1,100..900&family=Roboto+Mono:wght@400;500;600&display=swap"
      rel="stylesheet"
    />
    <link
      href="https://fonts.googleapis.com/css2?family=Mr+Dafoe&display=swap"
      rel="stylesheet"
    />

    <!-- Icons -->
    <link
      rel="stylesheet"
      href="https://fonts.googleapis.com/css2?family=Material+Symbols+Outlined:opsz,wght,FILL,GRAD@20..48,100..700,0..1,-50..200"
    />

    <!-- Styles -->
    <link rel="stylesheet" href="../css/index.css" />
    <link rel="stylesheet" href="../css/admin.css" />

    <!-- Scripts -->
    <script src="../js/index.js"></script>
    <script src="../js/admin.js" defer></script>

    <title>Natural Light Works</title>
  </head>
  <body>
    <main>
      <!-- Navbar -->
      <nav id="navbar">
        <h1 class="navbar-title cursive-font">Natural Light Works</h1>
        <button
          class="navbar-item button-primary"
          id="logout-button"
          onclick="logoutHandleOnClick()"
        >
          <span class="material-symbols-outlined icon-medium">logout</span>
          <span>Log out</span>
        </button>
        <button
          class="navbar-item button-primary"
          id="theme-button"
          onclick="themeHandleOnClick()"
        >
          <span class="material-symbols-outlined icon-medium">contrast</span>
          <span>Theme</span>
        </button>
      </nav>
    </main>
  </body>
</html>
